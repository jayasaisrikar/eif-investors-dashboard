{
  "version": 2,
  "name": "eif-investors-dashboard",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/public" }
    }
  ],
  "routes": [
    {
      "src": "/api/health$",
      "dest": "/api/health.ts"
    },
    {
      "src": "/api/auth/login$",
      "dest": "/api/auth/index.ts"
    },
    {
      "src": "/api/auth/logout$",
      "dest": "/api/auth/index.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/[...server].ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/(?!api/).*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
